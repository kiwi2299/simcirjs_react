<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>circuits.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardServlet</servlet-name>
        <servlet-class>circuits.DashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCircuit</servlet-name>
        <servlet-class>circuits.GetCircuit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveCircuit</servlet-name>
        <servlet-class>circuits.SaveCircuit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveCircuit</servlet-name>
        <servlet-class>circuits.RemoveCircuit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyCircuit</servlet-name>
        <servlet-class>circuits.ModifyCircuit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashboardServlet</servlet-name>
        <url-pattern>/DashboardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCircuit</servlet-name>
        <url-pattern>/GetCircuit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveCircuit</servlet-name>
        <url-pattern>/SaveCircuit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveCircuit</servlet-name>
        <url-pattern>/RemoveCircuit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyCircuit</servlet-name>
        <url-pattern>/ModifyCircuit</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
